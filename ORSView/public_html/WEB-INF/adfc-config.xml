<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="LoginPage">
    <page>/LoginPage.jsf</page>
  </view>
  <view id="SignUpPage">
    <page>/SignUpPage.jsf</page>
  </view>
  <view id="DashboardEmp">
    <page>/DashboardEmp.jsf</page>
  </view>
  <view id="DashboardCdate">
    <page>/DashboardCdate.jsf</page>
  </view>
  <view id="DashboardHR">
    <page>/DashboardHR.jsf</page>
  </view>
  <control-flow-rule id="__8">
    <from-activity-id>LoginPage</from-activity-id>
    <control-flow-case id="__5">
      <from-outcome>goToSignUp</from-outcome>
      <to-activity-id>SignUpPage</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__2">
      <from-outcome>goToCandidateDashboard</from-outcome>
      <to-activity-id>DashboardCdate</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__10">
      <from-outcome>goToEmployeeDashboard</from-outcome>
      <to-activity-id>DashboardEmp</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__13">
      <from-outcome>goToHRDashboard</from-outcome>
      <to-activity-id>DashboardHR</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__16">
    <from-activity-id>SignUpPage</from-activity-id>
    <control-flow-case id="__17">
      <from-outcome>goToCandidateDashboard</from-outcome>
      <to-activity-id>DashboardCdate</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__18">
      <from-outcome>goToLogin</from-outcome>
      <to-activity-id>LoginPage</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__7">
    <managed-bean-name>LoginPageBean</managed-bean-name>
    <managed-bean-class>oracle.demo.project.view.LoginPageBean</managed-bean-class>
    <managed-bean-scope>backingBean</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__1">
    <managed-bean-name>SignUpBean</managed-bean-name>
    <managed-bean-class>oracle.demo.project.view.SignUpBean</managed-bean-class>
    <managed-bean-scope>backingBean</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__4">
    <managed-bean-name>TestPageBean</managed-bean-name>
    <managed-bean-class>oracle.demo.project.view.TestPageBean</managed-bean-class>
    <managed-bean-scope>backingBean</managed-bean-scope>
  </managed-bean>
</adfc-config>
