<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="DashboardCdate">
    <page>/DashboardCdate.jsf</page>
  </view>
  <view id="Applications">
    <page>/Applications.jsf</page>
  </view>
  <view id="Interviews">
    <page>/Interviews.jsf</page>
  </view>
  <view id="JobPortal">
    <page>/JobPortal.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>dashboard</from-outcome>
      <to-activity-id>DashboardCdate</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__3">
      <from-outcome>applications</from-outcome>
      <to-activity-id>Applications</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__4">
      <from-outcome>interviews</from-outcome>
      <to-activity-id>Interviews</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>jobPortal</from-outcome>
      <to-activity-id>JobPortal</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
